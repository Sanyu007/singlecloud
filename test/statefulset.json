{
    "collectionUrl": "http://127.0.0.1:1234/apis/zcloud.cn/v1/clusters/sc-test-cluster1/namespaces/sc-test-namespace1/statefulsets",
    "resourceUrl": "http://127.0.0.1:1234/apis/zcloud.cn/v1/clusters/sc-test-cluster1/namespaces/sc-test-namespace1/statefulsets/sc-test-statefulset1",

    "params": {
        "name": "sc-test-statefulset1",
        "replicas": 2,
        "serviceName": "sc-test-sts-service1",
        "containers":      [
            {
                "name": "sc-test-containter1",
                "image": "redis",
                "command": ["ls"],
                "args": ["-l", "/"],
                "configName": "sc-test-configmap1",
                "mountPath": "/etc/scconfig",
                "secretName": "sc-test-secret1",
                "secretPath": "/etc/scsecret",
                "env": [
                    {
                        "name": "TESTENV1",
                        "value": "testenv1"
                    }
                ],
                "exposedPorts": [
                    {
                        "name": "sc-test-sts-port1",
                        "port": 22223,
                        "protocol": "tcp"
                    }
                ]
            }
        ],
        "advancedOptions": {
            "exposedServiceType": "clusterip",
            "exposedServices": [
                {
                    "name": "sc-test-sts-port1",
                    "port": 22223,
                    "protocol": "tcp",
                    "servicePort": 22223,
                    "autoCreateIngress": true,
                    "ingressDomainName": "sc-test-sts-ingress1",
                    "ingressPath": "/etc/scstsingress"
                }
            ]
        },
        "volumeClaimTemplate": {
            "storageSize": "100Mi",
            "storageClassName": "nfs"
        }
    }
}
