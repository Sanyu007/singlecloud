{
    "collectionUrl": "http://127.0.0.1:1234/apis/zcloud.cn/v1/clusters/sc-test-cluster1/namespaces/sc-test-namespace1/statefulsets",
    "resourceUrl": "http://127.0.0.1:1234/apis/zcloud.cn/v1/clusters/sc-test-cluster1/namespaces/sc-test-namespace1/statefulsets/sc-test-statefulset2",

    "params": {
        "name": "sc-test-statefulset2",
        "replicas": 2,
        "containers":      [
            {
                "name": "sc-test-containter1",
                "image": "redis",
                "command": ["ls"],
                "args": ["-l", "/"],
                "configName": "sc-test-configmap1",
                "mountPath": "/etc/scconfig",
                "secretName": "sc-test-secret1",
                "secretPath": "/etc/scsecret",
                "env": [
                    {
                        "name": "TESTENV1",
                        "value": "testenv1"
                    }
                ],
                "exposedPorts": [
                    {
                        "name": "sc-test-sts-port1",
                        "port": 22234,
                        "protocol": "tcp"
                    }
                ]
            }
        ],
        "advancedOptions": {
            "exposedServiceType": "clusterip",
            "exposedServices": [
                {
                    "name": "sc-test-sts-port2",
                    "port": 22224,
                    "protocol": "tcp",
                    "servicePort": 22224,
                    "autoCreateIngress": true,
                    "ingressDomainName": "sc-test-sts-ingress2.cluster.chengdu",
                    "ingressPath": "/etc/scstsingress",
                    "ingressPort": 33335
                }
            ]
        },
        "volumeClaimTemplate": {
            "name": "sc-test-emptydir1",
            "mountPath": "/etc/scststestpvc2",
            "storageSize": "100Mi",
            "storageClassName": "temporary"
        }
    }
}
